{
  "compilerOptions": {
    "outDir": "./src/views/",
    "module": "ES2022",
    "target": "ES2021",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "maxNodeModuleJsDepth": 0,
    "skipLibCheck": true,
    "jsx": "react",
  },
  "include": ["./src"],
}
